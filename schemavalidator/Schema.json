	"type": "object",
    "additionalProperties": False,
	"properties": {
		"note": {
			"type": "object",
			"properties": {
				"to": {
					"description": " Sending to",
					"type": "string"
				},
				"from": {
					"description": " Sender ",
					"type": "string"
				},
				"heading": {
					"description": " Subject ",
